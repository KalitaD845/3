    <ul>

    </ul>
    <script type="text/javascript">
     let  peoples = [{ name: "Женя", order: true}, { name: "Кристина", order: true }, { name: "Павел", order: false }, { name: "Виолетта", order: false },  {  name: "Костя",  order: true  }];
    
     for (let people of peoples ) {
         let li = document.createElement('li');
         li.textContent = 'Клиент'+ people.name + ( order.true ? ('Оплатил') : ('Отменил')) + 'заказ' ; 
         document.querySelector('ul').appendChild(li);
     }
     </script>
